page{
    background-color: #f5f5f5;
}
.mine-container{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.user-info-container{
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 160rpx;
}
.avatar{
    width: 200rpx;
    height: 200rpx;
}
.nickname{
    margin-top: 10rpx;
    font-size: 40rpx;
    font-weight: 600;
}
.position-container{
    margin-top: 10rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.province{
    font-size: 24rpx;
}
.divider{
    margin-left: 10rpx;
    margin-right: 10rpx;
    width: 1px;
    height: 22rpx;
    background-color: #999999;
}
.cache-container{
    margin-top: 40rpx;
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.cache-container .cache-img{
    width: 48rpx;
    height: 48rpx;
    margin-left: 10rpx;
}
.cache-text{
    margin-left: 20rpx;
    color: #333;
    font-size: 34rpx;
    font-weight: 500;
    line-height: 80rpx;
}